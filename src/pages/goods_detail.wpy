        padding-bottom: 5rpx;
        border-bottom: 1px solid #efefef;

      }
      .tags {
        .iconfont {
          font-size: 28rpx;
          color: #FA587E;
        }
        .label {
          font-size: 24rpx;
          margin: 0 10rpx;
        }
      }
    }
    &.comments, &.share-doc, &.detail-infos, &.image-text {
      .block-content {
        padding: 20rpx 22rpx;
        .process {
          font-size: 25rpx;
          margin: 0 auto;
          border: 1px solid #999999;
          padding: 10rpx;
          border-radius: 200px;
          text-align: center;
          margin-bottom: 25rpx;
          color: #808080;
        }
        .doc {
          color: #808080;
          font-size: 26rpx;
          line-height: 30rpx;
